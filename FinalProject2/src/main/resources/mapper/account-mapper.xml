<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="account">
	<delete id="deleteRewardAccount" parameterType="Map">
		DELETE FROM TB_REWARD_ACCOUNT WHERE FIN_NO=#{fintech_use_num} AND REWARD_NO=#{rewardNo}
	</delete>
	
	<insert id="insertRewardAccount" parameterType="Map">
		INSERT INTO TB_REWARD_ACCOUNT VALUES(#{fintech_use_num},#{rewardNo},#{account_num_masked},#{bank_code_std},#{bank_name},#{user_token},#{user_refresh_token},#{user_seq_no},#{account_holder_name},#{account_alias})
	</insert>
	
</mapper>
